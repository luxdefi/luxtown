{
  "name": "@luxdefi/luxtown",
  "version": "1.0.0",
  "author": "Lux Financial Limited <hi@lux.financial> (https://lux.financial)",
  "license": "MIT",
  "keywords": [
    "ethereum",
    "react",
    "uniswap",
    "workspaces",
    "yarn"
  ],
  "private": true,
  "scripts": {
    "build": "yarn build:app",
    "build:all":"yarn build:sdk && yarn build:app",
    "build:app": "yarn workspace @luxdefi/app build",
    "build:lab": "yarn workspace @luxdefi/lab build",
    "build:sdk": "yarn workspace @luxdefi/sdk build",
    "clean": "rm -rf packages/app/build && rm -rf packages/contracts/artifacts && rm -rf packages/contracts/cache && rm -rf packages/contracts/types",
    "dev": "yarn build:sdk && yarn workspace @luxdefi/app dev",
    "console": "yarn workspace @luxdefi/contracts console",
    "upload": "yarn workspace @luxdefi/moralis upload",
    "uploaded": "yarn workspace @luxdefi/moralis uploaded",
    "start": "yarn workspace @luxdefi/app start",
    "start:lab": "yarn workspace @luxdefi/lab start",
    "account": "yarn workspace @luxdefi/contracts account",
    "accounts": "yarn workspace @luxdefi/contracts accounts",
    "balance": "yarn workspace @luxdefi/contracts balance",
    "chain": "yarn workspace @luxdefi/contracts chain",
    "chain2": "yarn workspace @luxdefi/contracts chain2",
    "compile": "yarn workspace @luxdefi/contracts compile",
    "deploy": "yarn workspace @luxdefi/contracts deploy",
    "deploy:game": "yarn workspace @luxdefi/contracts deploy:game",
    "deploy:mainnet": "yarn workspace @luxdefi/contracts deploy:mainnet",
    "deploy:testnet": "yarn workspace @luxdefi/contracts deploy:testnet",
    "configure": "yarn workspace @luxdefi/contracts configure",
    "configure:testnet": "yarn workspace @luxdefi/contracts configure:testnet",
    "configure:mainnet": "yarn workspace @luxdefi/contracts configure:mainnet",
    "export": "yarn workspace @luxdefi/sdk build:sdk && yarn workspace @luxdefi/contracts export",
    "flatten": "yarn workspace @luxdefi/contracts flatten",
    "fork": "yarn workspace @luxdefi/contracts fork",
    "fundedwallet": "yarn workspace @luxdefi/contracts fundedwallet",
    "generate": "yarn workspace @luxdefi/contracts generate",
    "matrix": "yarn workspace @luxdefi/contracts matrix",
    "mineContractAddress": "yarn workspace @luxdefi/contracts mineContractAddress",
    "node": "yarn workspace @luxdefi/contracts chain",
    "send": "yarn workspace @luxdefi/contracts send",
    "test": "yarn workspace @luxdefi/contracts test",
    "wallet": "yarn workspace @luxdefi/contracts wallet",
    "drop": "yarn workspace @luxdefi/moralis drop",
    "functions": "yarn workspace @luxdefi/moralis functions",
    "logs": "yarn workspace @luxdefi/moralis logs",
    "plugins": "yarn workspace @luxdefi/moralis plugins",
    "proxy": "yarn workspace @luxdefi/moralis proxy",
    "watch": "yarn workspace @luxdefi/moralis watch",
    "lab": "yarn workspace @luxdefi/lab start",
    "subgraph:codegen": "yarn workspace @luxdefi/subgraph codegen",
    "subgraph:prepare-local": "yarn workspace @luxdefi/subgraph prepare:local",
    "subgraph:prepare-testnet": "yarn workspace @luxdefi/subgraph prepare:testnet",
    "subgraph:prepare-mainnet": "yarn workspace @luxdefi/subgraph prepare:mainnet",
    "subgraph:build": "yarn workspace @luxdefi/subgraph build",
    "subgraph:create-local": "yarn workspace @luxdefi/subgraph create:local",
    "subgraph:deploy-local": "yarn workspace @luxdefi/subgraph deploy:local",
    "subgraph:deploy-testnet": "yarn workspace @luxdefi/subgraph deploy:testnet",
    "subgraph:deploy-mainnet": "yarn workspace @luxdefi/subgraph deploy:mainnet",
    "subgraph:start": "yarn workspace @luxdefi/subgraph start",
    "graphnode:start": "yarn workspace @luxdefi/graphnode start",
    "graphnode:docker": "yarn workspace @luxdefi/graphnode docker",
    "graphnode:clean": "yarn workspace @luxdefi/graphnode clean",
    "format": "yarn workspace @luxdefi/app format && yarn workspace @luxdefi/contracts format && yarn workspace @luxdefi/moralis format",
    "verify:testnet": "yarn workspace @luxdefi/contracts verify:testnet"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/@openzeppelin/contracts-upgradeable/**",
      "**/@openzeppelin/contracts",
      "**/@luxdefi/framework",
      "**/@luxdefi/solidity"
    ]
  },
  "resolutions": {
    "node-hid": "./resolutions/node-hid"
  }
}
